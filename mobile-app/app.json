{
  "expo": {
    "name": "Financial Security",
    "slug": "financial-security-app",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1a237e"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.financialsecurity.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1a237e"
      },
      "package": "com.financialsecurity.app",
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {},
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera for QR code scanning."
        }
      ]
    ],
    "extra": {
      "apiUrl": "http://192.168.178.33:8000"
    }
  }
}
