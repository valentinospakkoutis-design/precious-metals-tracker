		{
			"name": "Two-Factor Authentication (2FA)",
			"item": [
				{
					"name": "Enable 2FA",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"2FA enabled\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData).to.have.property('secret');",
									"    pm.expect(jsonData).to.have.property('qr_code');",
									"    pm.expect(jsonData).to.have.property('backup_codes');",
									"    pm.environment.set(\"two_factor_secret\", jsonData.secret);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"url": {
							"raw": "{{base_url}}/api/v1/auth/2fa/enable",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "auth", "2fa", "enable"]
						},
						"description": "Enable 2FA and receive QR code + backup codes"
					},
					"response": []
				},
				{
					"name": "Verify 2FA Setup",
