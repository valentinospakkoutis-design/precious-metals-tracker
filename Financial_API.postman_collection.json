						"url": {
							"raw": "{{base_url}}/api/v1/auth/2fa/backup-code",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "auth", "2fa", "backup-code"]
						},
						"description": "Emergency login with backup code"
					},
					"response": []
				}
