						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"secret\": \"{{two_factor_secret}}\",\n    \"token\": \"123456\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/auth/2fa/verify",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "auth", "2fa", "verify"]
						},
						"description": "Verify 2FA setup with first TOTP token"
					},
					"response": []
				},
				{
					"name": "Login with 2FA",
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"email\": \"{{test_email}}\",\n    \"password\": \"{{test_password}}\",\n    \"totp_token\": \"123456\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/auth/login/2fa",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "auth", "login", "2fa"]
						},
						"description": "Login with email, password, and TOTP token"
					},
					"response": []
				},
				{
					"name": "Disable 2FA",
