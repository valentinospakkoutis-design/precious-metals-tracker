						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"totp_token\": \"123456\"\n}"
						},
						"url": {
							"raw": "{{base_url}}/api/v1/auth/2fa/disable",
							"host": ["{{base_url}}"],
							"path": ["api", "v1", "auth", "2fa", "disable"]
						},
						"description": "Disable 2FA (requires current TOTP token)"
					},
					"response": []
				},
				{
					"name": "Login with Backup Code",
					"request": {
						"auth": {
							"type": "noauth"
